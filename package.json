  "scripts": {
    "build": "ng build",
    "build-dev": "ng build -c dev",
    "build-local": "ng build -c local",
    "build-prod": "ng build --prod",
    "check": "prettier --check '**/*.{ts,json,scss,html}'",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "node --max-old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "start": "ng serve",
    "start:dev": "ng serve -o -c dev",
    "start:local": "ng serve -o -c local",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:watch": "ng test --watch --browsers=ChromeHeadless --code-coverage",
    "watch": "ng build --watch --configuration dev"
  },
